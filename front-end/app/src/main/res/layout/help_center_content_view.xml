            android:textColor="@color/black" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:text="@string/anggota_tentang_aplikasi_2"
            android:textSize="12sp"
            android:textColor="@color/black" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:text="@string/anggota_tentang_aplikasi_3"
            android:textSize="12sp"
            android:textColor="@color/black" />
        <TextView
            android:layout_width="match_parent"
